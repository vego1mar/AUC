<?xml version="1.0" encoding="UTF-8"?>

<rulesSet>
  <rule no="1">
    <target>
      <in>CODE</in>
      <useAs>IGNORE</useAs>
    </target>
    <method>
      <methodType>RETRIEVE_TAGS</methodType>
      <type>FIRST</type>
      <tagname>title</tagname>
    </method>
  </rule>
  <rule no="2">
    <target>
      <in>CONTENT</in>
      <useAs>IGNORE</useAs>
    </target>
    <method>
      <methodType>EXTRACT_WORD</methodType>
      <position>3</position>
    </method>
  </rule>
  <rule no="3">
    <target>
      <in>CONTENT</in>
      <useAs>VERSIONS</useAs>
      <version>WINDOWS</version>
    </target>
    <method>
      <methodType>TRIM</methodType>
      <side>RIGHT</side>
      <numberOf>8</numberOf>
    </method>
  </rule>
  <rule no="4">
    <target>
      <in>CODE</in>
      <useAs>IGNORE</useAs>
    </target>
    <method>
      <methodType>FIRST_OF</methodType>
      <type>STRING</type>
      <text><![CDATA[Debug:]]></text>
    </method>
  </rule>
  <rule no="5">
    <target>
      <in>CONTENT</in>
      <useAs>IGNORE</useAs>
    </target>
    <method>
      <methodType>TRIM</methodType>
      <side>LEFT</side>
      <numberOf>7</numberOf>
    </method>
  </rule>
  <rule no="6">
    <target>
      <in>CONTENT</in>
      <useAs>IGNORE</useAs>
    </target>
    <method>
      <methodType>GRAB_UNTIL</methodType>
      <charStop><![CDATA[>]]></charStop>
    </method>
  </rule>
  <rule no="7">
    <target>
      <in>CONTENT</in>
      <useAs>LINKS</useAs>
      <linkId>WINDOWS_X86_EXE</linkId>
    </target>
    <method>
      <methodType>TRIM</methodType>
      <side>RIGHT</side>
      <numberOf>3</numberOf>
    </method>
  </rule>
</rulesSet>
