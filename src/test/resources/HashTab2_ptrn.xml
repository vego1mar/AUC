<?xml version="1.0" encoding="UTF-8"?>

<rulesSet>
  <rule no="1">
    <target>
      <in>CODE</in>
      <useAs>IGNORE</useAs>
    </target>
    <method>
      <methodType>FIRST_OF</methodType>
      <type>STRING</type>
      <text><![CDATA[<meta itemprop="datePublished"]]></text>
    </method>
  </rule>
  <rule no="2">
    <target>
      <in>CONTENT</in>
      <useAs>IGNORE</useAs>
    </target>
    <method>
      <methodType>TRIM</methodType>
      <side>LEFT</side>
      <numberOf>30</numberOf>
    </method>
  </rule>
  <rule no="3">
    <target>
      <in>CONTENT</in>
      <useAs>IGNORE</useAs>
    </target>
