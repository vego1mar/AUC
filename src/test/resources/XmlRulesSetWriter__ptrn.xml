<?xml version="1.0" encoding="UTF-8"?>

<rulesSet>
  <rule no="1">
    <target>
      <in>CODE</in>
      <useAs>IGNORE</useAs>
    </target>
    <method>
      <methodType>EMPTY</methodType>
    </method>
  </rule>
  <rule no="2">
    <target>
      <in>CONTENT</in>
      <useAs>VERSIONS</useAs>
      <version>WINDOWS</version>
    </target>
    <method>
      <methodType>FIRST_OF</methodType>
      <type>STRING</type>
      <text><![CDATA[ \xBA ]]></text>
    </method>
  </rule>
  <rule no="3">
    <target>
      <in>COLLECTION</in>
      <useAs>DATES</useAs>
      <date>ANDROID</date>
    </target>
    <method>
      <methodType>EXTRACT_WORD</methodType>
      <position>1</position>
    </method>
  </rule>
  <rule no="4">
    <target>
      <in>CODE</in>
      <useAs>LINKS</useAs>
      <linkId>SOURCECODE_ANY_7Z</linkId>
    </target>
    <method>
      <methodType>FETCH_HREFS</methodType>
    </method>
  </rule>
  <rule no="5">
    <target>
      <in>CONTENT</in>
      <useAs>HASHES</useAs>
      <hashId>ANDROID_X86ARM_APK</hashId>
    </target>
    <method>
      <methodType>GRAB_UNTIL</methodType>
      <charStop><![CDATA[ ]]></charStop>
    </method>
  </rule>
  <rule no="6">
    <target>
      <in>CONTENT</in>
      <useAs>IGNORE</useAs>
    </target>
    <method>
      <methodType>PREPEND</methodType>
      <text><![CDATA[ aąbcćdeęfghijklłmnńoópqrsśtuwxyzżź0123456789 `~!@#$%^&*()_+-=[]\;':",./<>? ]]></text>
    </method>
  </rule>
  <rule no="7">
    <target>
      <in>CONTENT</in>
      <useAs>IGNORE</useAs>
    </target>
    <method>
      <methodType>REMOVE_CHARACTERS</methodType>
      <signs><![CDATA[ Mężny bądź, chroń pułk swój i sześć flag.]]></signs>
    </method>
  </rule>
  <rule no="8">
    <target>
      <in>CONTENT</in>
      <useAs>IGNORE</useAs>
    </target>
    <method>
      <methodType>RETRIEVE_TAGS</methodType>
      <type>ALL</type>
      <tagname>Ж</tagname>
    </method>
  </rule>
  <rule no="9">
    <target>
      <in>CONTENT</in>
      <useAs>IGNORE</useAs>
    </target>
    <method>
      <methodType>SPLIT_WORDS</methodType>
    </method>
  </rule>
  <rule no="10">
    <target>
      <in>CONTENT</in>
      <useAs>IGNORE</useAs>
    </target>
    <method>
      <methodType>TRIM</methodType>
      <side>LEFT</side>
      <numberOf>0</numberOf>
    </method>
  </rule>
</rulesSet>
