<?xml version="1.0" encoding="UTF-8"?>

<rulesSet>
  <rule no="1">
    <target>
      <in>CODE</in>
      <useAs>IGNORE</useAs>
    </target>
    <method>
      <methodType>FIRST_OF</methodType>
      <type>TAG</type>
      <text><![CDATA[tbody]]></text>
    </method>
  </rule>
  <rule no="2">
    <target>
      <in>CONTENT</in>
      <useAs>IGNORE</useAs>
    </target>
    <method>
      <methodType>FIRST_OF</methodType>
      <type>TAG</type>
      <text><![CDATA[tr]]></text>
    </method>
  </rule>
  <rule no="3">
    <target>
      <in>CONTENT</in>
      <useAs>IGNORE</useAs>
    </target>
    <method>
      <methodType>RETRIEVE_TAGS</methodType>
      <type>ALL</type>
      <tagname>td</tagname>
    </method>
  </rule>
  <rule no="4">
    <target>
      <in>COLLECTION</in>
      <useAs>IGNORE</useAs>
    </target>
    <method>
      <methodType>EXTRACT_WORD</methodType>
      <position>1</position>
    </method>
  </rule>
  <rule no="5">
    <target>
      <in>CONTENT</in>
      <useAs>IGNORE</useAs>
    </target>
    <method>
      <methodType>FIRST_OF</methodType>
      <type>STRING</type>
      <text><![CDATA[ ]]></text>
    </method>
  </rule>
  <rule no="6">
    <target>
      <in>CONTENT</in>
      <useAs>IGNORE</useAs>
    </target>
    <method>
      <methodType>GRAB_UNTIL</methodType>
      <charStop><![CDATA[<]]></charStop>
    </method>
  </rule>
  <rule no="7">
    <target>
      <in>CONTENT</in>
      <useAs>VERSIONS</useAs>
      <version>WINDOWS</version>
    </target>
    <method>
      <methodType>TRIM</methodType>
      <side>LEFT</side>
      <numberOf>1</numberOf>
    </method>
  </rule>
  <rule no="8">
    <target>
      <in>COLLECTION</in>
      <useAs>IGNORE</useAs>
    </target>
    <method>
      <methodType>EXTRACT_WORD</methodType>
      <position>3</position>
    </method>
  </rule>
  <rule no="9">
    <target>
      <in>CONTENT</in>
      <useAs>IGNORE</useAs>
    </target>
    <method>
      <methodType>FETCH_HREFS</methodType>
    </method>
  </rule>
  <rule no="10">
    <target>
      <in>CONTENT</in>
      <useAs>LINKS</useAs>
      <linkId>WINDOWS_X86_EXE</linkId>
    </target>
    <method>
      <methodType>PREPEND</methodType>
      <text><![CDATA[http://www.codesector.com]]></text>
    </method>
  </rule>
</rulesSet>
